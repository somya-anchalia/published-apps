<dashboard version="1.1"
           script="application_configuration_webpack.js, asa_init.js"
           stylesheet="app_configuration.css">
    <label>Application Configuration</label>
    <row>
        <panel>
            <html>
                <div class="documentation_box">
                    <dl>
                        <dt>Application Configuration</dt>
                        <dd>This tab contains the "Save" button to show that the App was configured. It may also contain
                            information and configurations relating to the data.
                        </dd>
                    </dl>
                </div>
            </html>
        </panel>
    </row>
    <row>
        <panel>
            <html>
                <div id="button_container" class="button_container"></div>
            </html>
        </panel>
    </row>
    <row id="row_tabs">
        <panel>
            <html>
                <ul id="tabs" class="nav nav-tabs"></ul>
                <div class="message_box" id="msg_box" name="msg_box"></div>
                <div class="slide_pills"></div>
                <div id="tab_content_container" class="tab_content_container"></div>
            </html>
        </panel>
    </row>
</dashboard>